{
    "name": "Seonbi",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Seonbi",
    "icons": {
        "16": "icons/icon-16.png",
        "128": "icons/icon-128.png"
    },
    "permissions": [
        "https://twitter.com/*",
        "webRequest"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://twitter.com/*"
            ],
            "js": [
                "contentscript.js"
            ],
            "run_at": "document_start",
            "all_frames": false
        }
    ],
    "background": {
        "scripts": ["background.js"]
    }
}